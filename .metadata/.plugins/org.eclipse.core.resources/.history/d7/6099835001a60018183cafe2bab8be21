	public Pup getName(Pup Name) {
		
		return Name; 
	}
	public Pup remove(Pup Name) {
		return Name;
	}
	 public String position(int pos) {
		 String ret = "";
		 int pup = 4;
//			ArrayList<Integer> dog = new ArrayList<Integer>();
		 int ar [] = new int [100]; 
					
			for(int i = 1; i <101; i++)
			{	
				if(ar[i]!= pos) {
					ar[i-1]= i;
					ret +=ar[i-1];
				}
				
			}
		 return ret;
	 }
}
