
public class HelloWorld {

	public static void main(String[] args) {
		int sum; boolean whatever = true;
		String i = "world";
		
		System.out.println("hello");
		
		System.out.println(i);
		
		System.out.println(helloWorld(i));
		
		System.out.println(retMethod());
		
		sum = sumMethod(12,13, whatever);
		System.out.println(sumMethod(12, 13, true));
		
	}
	
	public static String helloWorld(String i) {
	
		String k = "mcworldface";
		
		return i +" "+ k;
		
	}
	
	public static String retMethod() {
		
	String j = "Hi";	
		
	return j;	
		
	}
	
	public static int sumMethod(int f, int g, boolean con) {
		
		 int sum;
		if (con == true) {
			sum =  f + g;	
		}
		
		else {
			sum = f * g;
		}
		return sum;

		
	}
	
	

}
